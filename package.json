{
  "name": "@mels/react-components",
  "version": "0.1.4",
  "description": "A collection of some of my frequently used React components.",
  "author": "Melanie Seltzer <melleh11@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/melanieseltzer/react-components#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/melanieseltzer/react-components.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "lint": "eslint --ignore-path .eslintignore --ext .ts --ext .tsx src",
    "prepare": "yarn run build"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.0.5",
    "@types/styled-components": "^4.1.14",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "cross-env": "^5.1.4",
    "docz": "^1.1.0",
    "docz-plugin-netlify": "^1.1.4",
    "docz-theme-default": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.16.4",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hot-loader": "^4.8.4",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.11.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.21.0",
    "rollup-plugin-url": "^2.2.0",
    "styled-components": "^4.2.0",
    "typescript": "^3.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "prettier --write",
        "eslint",
        "git add"
      ]
    }
  },
  "files": [
    "dist"
  ]
}
